@model ApplicationUser
@{
    ViewData["Title"] = "Delete";
}

<h1>Are you sure you want to delete this user?</h1>

<form asp-action="Delete" enctype="multipart/form-data" class="mt-3">
    <input hidden asp-for="Id" />
    <partial name="ParitialViews/DetailsDeleteParitialView" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index" asp-controller="Users" class="btn btn-light">Back to Lists</a><br />
</form>

