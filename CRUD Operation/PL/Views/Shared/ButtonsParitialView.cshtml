@model string

<td class="align-middle">
    <a asp-action="Details" asp-controller="Employee" class="btn btn-success" asp-route-id="@Model">
        <i class="fas fa-eye"></i>  Details
    </a>
</td>
<td class="align-middle">
    <a asp-action="Edit" asp-controller="Employee" class="btn btn-warning" asp-route-id="@Model">
        <i class="fas fa-edit"></i>  Update
    </a>
</td>
<td class="align-middle">
    <a asp-action="Delete" asp-controller="Employee" class="btn btn-danger" asp-route-id="@Model">
        <i class="fas fa-trash"></i>  Delete
    </a>
</td>
